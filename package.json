    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1"
